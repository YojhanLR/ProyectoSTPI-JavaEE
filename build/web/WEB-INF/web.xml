<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletPrueba</servlet-name>
        <servlet-class>com.stpi.controller.ServletPrueba</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductorUpdate</servlet-name>
        <servlet-class>com.stpi.controller.ConductorUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductorEdit</servlet-name>
        <servlet-class>com.stpi.controller.ConductorEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductorIndex</servlet-name>
        <servlet-class>com.stpi.controller.ConductorIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductorCreate</servlet-name>
        <jsp-file>/views/Administrador/Conductores/create.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ConductorStore</servlet-name>
        <servlet-class>com.stpi.controller.ConductorStore</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductoDestroy</servlet-name>
        <servlet-class>com.stpi.controller.ConductoDestroy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConductorDestroy</servlet-name>
        <servlet-class>com.stpi.controller.ConductorDestroy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPrueba</servlet-name>
        <url-pattern>/ServletPrueba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorUpdate</servlet-name>
        <url-pattern>/Conductores/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorEdit</servlet-name>
        <url-pattern>/Conductores/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorIndex</servlet-name>
        <url-pattern>/Conductores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorCreate</servlet-name>
        <url-pattern>/Conductores/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorStore</servlet-name>
        <url-pattern>/Conductores/store</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConductorDestroy</servlet-name>
        <url-pattern>/Conductores/destroy</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
